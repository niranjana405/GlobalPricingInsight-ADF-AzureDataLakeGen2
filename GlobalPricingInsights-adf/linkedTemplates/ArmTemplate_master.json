{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "GlobalPricingInsights-adf"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"LS_GEOLOCATION_DATALAKE_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_GEOLOCATION_DATALAKE'"
		},
		"LS_sink_dailypricing_lab1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_sink_dailypricing_lab1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://globalpricingdatagen2.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://globalpricingdatagen2.dfs.core.windows.net/"
		},
		"LS_GEOLOCATION_DATALAKE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://globalpricingdatagen2.dfs.core.windows.net/"
		},
		"LS_HTTP_GEOLOCATION_SOURCE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://geocoding-api.open-meteo.com/"
		},
		"LS_HTTP_WEATHER_SOURCE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://archive-api.open-meteo.com/"
		},
		"Ls_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://retailpricing.blob.core.windows.net"
		},
		"TWTRG_daily_pricing_properties_pipeline_parameters_dst_source_file_date": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"TWTRG_MONTHLY_WEATHER_DATA_properties_pipeline_parameters_pln_prm_weather_start_date": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"TWTRG_MONTHLY_WEATHER_DATA_properties_pipeline_parameters_pln_prm_weather_end_date": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"TWTRG_MONTHLY_WEATHER_DATA_properties_pipeline_parameters_pln_prm_sink_output_format": {
			"type": "string",
			"defaultValue": "json"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "GlobalPricingInsights-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"LS_GEOLOCATION_DATALAKE_accountKey": {
						"value": "[parameters('LS_GEOLOCATION_DATALAKE_accountKey')]"
					},
					"LS_sink_dailypricing_lab1_connectionString": {
						"value": "[parameters('LS_sink_dailypricing_lab1_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"LS_GEOLOCATION_DATALAKE_properties_typeProperties_url": {
						"value": "[parameters('LS_GEOLOCATION_DATALAKE_properties_typeProperties_url')]"
					},
					"LS_HTTP_GEOLOCATION_SOURCE_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTP_GEOLOCATION_SOURCE_properties_typeProperties_url')]"
					},
					"LS_HTTP_WEATHER_SOURCE_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTP_WEATHER_SOURCE_properties_typeProperties_url')]"
					},
					"Ls_source_properties_typeProperties_url": {
						"value": "[parameters('Ls_source_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "GlobalPricingInsights-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'GlobalPricingInsights-adf_ArmTemplate_0')]"
			]
		},
		{
			"name": "GlobalPricingInsights-adf_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"TWTRG_daily_pricing_properties_pipeline_parameters_dst_source_file_date": {
						"value": "[parameters('TWTRG_daily_pricing_properties_pipeline_parameters_dst_source_file_date')]"
					},
					"TWTRG_MONTHLY_WEATHER_DATA_properties_pipeline_parameters_pln_prm_weather_start_date": {
						"value": "[parameters('TWTRG_MONTHLY_WEATHER_DATA_properties_pipeline_parameters_pln_prm_weather_start_date')]"
					},
					"TWTRG_MONTHLY_WEATHER_DATA_properties_pipeline_parameters_pln_prm_weather_end_date": {
						"value": "[parameters('TWTRG_MONTHLY_WEATHER_DATA_properties_pipeline_parameters_pln_prm_weather_end_date')]"
					},
					"TWTRG_MONTHLY_WEATHER_DATA_properties_pipeline_parameters_pln_prm_sink_output_format": {
						"value": "[parameters('TWTRG_MONTHLY_WEATHER_DATA_properties_pipeline_parameters_pln_prm_sink_output_format')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'GlobalPricingInsights-adf_ArmTemplate_1')]"
			]
		}
	]
}